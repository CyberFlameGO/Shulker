{
  "root": "packages/shulker-operator",
  "sourceRoot": "packages/shulker-operator/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o='../../dist/packages/shulker-operator/' ./src/main.go",
        "cwd": "packages/shulker-operator"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./src/main.go",
        "cwd": "packages/shulker-operator"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "go fmt ./...",
          "go vet ./..."
        ],
        "cwd": "packages/shulker-operator"
      }
    }
  }
}
